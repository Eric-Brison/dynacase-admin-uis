<html>
    <head>
        <title>[app_short_name]</title>
        [BLOCK CSS_LINKS]
        <link href="[src]" type="text/css" rel="stylesheet"></link>
        [ENDBLOCK CSS_LINKS]
        [BLOCK JS_LINKS]
        <script type="text/javascript" src="[src]"></script>
        [ENDBLOCK JS_LINKS]
    </head>
    <body>

        <table align="center" width="100%" border="0" cellspacing="0"
               cellpadding="0" id="dataTable">

            <!-- TABLEHEAD -->
            <thead>
                <tr>
                    <th class="ui-widget-header id">
                        <a id="updateAll" title="[TEXT:UpdateAll]">
                        </a>
                    </th>
                    <th class="ui-widget-header name">
                        <span class="CORETblBold">[TEXT:appname]</span>
                    </th>
                    <th class="ui-widget-header version">
                        <span class="CORETblBold">[TEXT:Version]</span>
                    </th>
                    <th class="ui-widget-header description">
                        <span class="CORETblBold">[TEXT:description]</span>
                    </th>
                    <th class="ui-widget-header available">
                        <span class="CORETblBold">[TEXT:available]</span>
                    </th>
                    <th class="ui-widget-header displayable">
                        <span class="CORETblBold">[TEXT:displayable]</span>
                    </th>
                </tr>
            </thead>
            <!-- /TABLEHEAD -->
            <tbody>
            </tbody>
        </table>
        <div class="ui-widget-overlay" style="display:none;"></div>
    </body>
</html>