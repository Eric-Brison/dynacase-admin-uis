<div class="do" id="DGroups" style="height:100%">
<UL id="gtreeall" class="mktree">
    [BLOCK ALL]
    <LI name="LI[id]">
        <img title="[TEXT:view detail] ([login])" src="[icon]"
             width="14px" />
        <a onclick="[onclick]" href="#">
            <span title="[login]" id="SPAN[id]" name="SPAN[id]">[firstname]
            </span>
        </a>
    </LI>
    [ENDBLOCK ALL]
</UL>
<div style="width:100%;height:1px;overflow:hidden;clear:both;border-top:#CCC solid 1px"></div>
<UL id="gtree" class="mktree">
    [BLOCK LI]
    <LI name="LI[id]">
        <img class="click" title="[TEXT:view detail] ([login])" src="[icon]"
             width="14px" onclick="[onclickimg]"/>
        <a onclick="[onclick]" href="#">
            <span title="[login]" id="SPAN[id]" name="SPAN[id]">[firstname]
                [lastname]
            </span>
        </a>
        [SUBUL]
    </LI>
    [ENDBLOCK LI]
</UL>
</div>
